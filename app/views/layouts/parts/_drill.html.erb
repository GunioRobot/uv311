<%= search %>
<% if search %>
<div id="search">
  <div class="container">
      <% render :partial=> '/issues/search' %>
  </div>
</div>
<% end %>
<div id="content" class="span-38">
    <div id="<%=left_column_id%>" class="span-<%=(columns-1)%> prepend-1 append-1">
      <% # errors here -%>
      <%= render_flash_messages %>
      <%= render :partial => quad_index %>
    </div>
    <div id="<%=right_column_id%>" class="span-<%=right_column%> last">
      
      <%# if local is not passed partial is ignored %>
      <%= render :partial => quad_column %>
      
    </div>
</div>
